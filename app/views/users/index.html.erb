<h2>All Users</h2>
<table>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>



<%= link_to 'create new user', signup_path %>
<%= render 'sessions/logout' %>